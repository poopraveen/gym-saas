{
  "name": "gym-saas",
  "version": "1.0.0",
  "description": "Multi-tenant Gym Management SaaS - NestJS + RBAC",
  "scripts": {
    "build": "nest build",
    "generate-vapid-keys": "node -e \"const w=require('web-push'); const k=w.generateVAPIDKeys(); console.log('VAPID_PUBLIC_KEY='+k.publicKey); console.log('VAPID_PRIVATE_KEY='+k.privateKey);\"",
    "tunnel": "ngrok http 3000",
    "free-port": "npx --yes kill-port 3000",
    "dev:telegram": "node scripts/telegram-tunnel.mjs",
    "seed": "ts-node src/scripts/seed.ts",
    "reset-admin": "ts-node src/scripts/reset-admin.ts",
    "reset-trainer": "ts-node src/scripts/reset-trainer.ts",
    "check:telegram": "ts-node src/scripts/check-telegram-tenants.ts",
    "cleanup-duplicate-member": "ts-node src/scripts/cleanup-duplicate-member.ts",
    "cleanup-duplicate-reg-no": "ts-node src/scripts/cleanup-duplicate-reg-no.ts",
    "import:gym-users": "ts-node src/scripts/import-gym-users.ts",
    "capture-pitch-screenshots": "node scripts/capture-pitch-screenshots.mjs",
    "start": "node dist/main",
    "start:dev": "nest start --watch",
    "dev:client": "cd client && npm run dev",
    "dev": "concurrently -n api,web -c blue,green \"npm run start:dev\" \"npm run dev:client\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/mongoose": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@types/multer": "^2.0.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cloudinary": "^2.9.0",
    "mongoose": "^8.0.0",
    "multer": "^2.0.2",
    "openai": "^6.17.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pdfkit": "^0.17.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sharp": "^0.34.5",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.0",
    "@types/web-push": "^3.6.4",
    "concurrently": "^9.1.0",
    "playwright": "^1.58.1",
    "ts-node": "^10.9.0",
    "typescript": "^5.1.3"
  }
}
